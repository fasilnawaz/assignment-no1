import React from 'react';
import { View, Text, Button, StyleSheet, FlatList, TouchableOpacity, TextInput, Image } from 'react-native';
import { DarkTheme, Provider as PaperProvider } from 'react-native-paper';
import { NavigationContainer } from '@react-navigation/native';
import { createStackNavigator } from '@react-navigation/stack';

const Stack = createStackNavigator();

// Mock Data with Online Images
const restaurants = [
  {
    id: '1',
    name: 'Pizza Place',
    rating: 4.5,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
  {
    id: '2',
    name: 'Burger Joint',
    rating: 4.0,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
  {
    id: '3',
    name: 'Sushi Bar',
    rating: 4.7,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
];

const menuItems = [
  {
    id: '1',
    name: 'Margherita Pizza',
    price: 12,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
  {
    id: '2',
    name: 'Cheeseburger',
    price: 8,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
  {
    id: '3',
    name: 'Sushi Rolls',
    price: 15,
    image: { uri: 'data:image/jpeg;base64,' }, // Replace with your image URL
  },
];

// Home Screen UI: Show List of Restaurants
const HomeScreen = ({ navigation }) => {
  return (
    <View style={styles.container}>
      <Text style={styles.title}>Restaurants</Text>
      <FlatList
        data={restaurants}
        renderItem={({ item }) => (
          <TouchableOpacity onPress={() => navigation.navigate('RestaurantDetails', { restaurantId: item.id })}>
            <View style={styles.card}>
              <Image source={item.image} style={styles.cardImage} />
              <Text style={styles.cardTitle}>{item.name}</Text>
              <Text style={styles.cardText}>Rating: {item.rating}</Text>
            </View>
          </TouchableOpacity>
        )}
        keyExtractor={item => item.id}
      />
    </View>
  );
};

// Restaurant Details Screen UI: Menu Items
const RestaurantDetailsScreen = ({ route, navigation }) => {
  const { restaurantId } = route.params;

  return (
    <View style={styles.container}>
      <Text style={styles.title}>Menu for Restaurant {restaurantId}</Text>
      <FlatList
        data={menuItems}
        renderItem={({ item }) => (
          <View style={styles.card}>
            <Image source={item.image} style={styles.menuImage} />
            <Text style={styles.menuItemName}>{item.name} - ${item.price}</Text>
            <Button title="Add to Cart" onPress={() => {}} />
          </View>
        )}
        keyExtractor={item => item.id}
      />
      <Button title="Go to Cart" onPress={() => navigation.navigate('Cart')} />
    </View>
  );
};

// Cart Screen UI: Review Cart
const CartScreen = ({ navigation }) => {
  return (
    <View style={styles.container}>
      <Text style={styles.title}>Your Cart</Text>
      <Text style={styles.cardText}>No items in the cart yet.</Text>
      <Button title="Proceed to Checkout" onPress={() => navigation.navigate('Checkout')} />
    </View>
  );
};

// Checkout Screen UI: Enter Delivery Address
const CheckoutScreen = ({ navigation }) => {
  const [address, setAddress] = React.useState('');

  return (
    <View style={styles.container}>
      <Text style={styles.title}>Checkout</Text>
      <TextInput
        style={styles.input}
        placeholder="Enter Delivery Address"
        placeholderTextColor="#999"
        value={address}
        onChangeText={setAddress}
      />
      <Button title="Place Order" onPress={() => navigation.navigate('OrderTracking')} />
    </View>
  );
};

// Order Tracking Screen UI: Track Order
const OrderTrackingScreen = () => {
  return (
    <View style={styles.container}>
      <Text style={styles.title}>Order Tracking</Text>
      <Text style={styles.cardText}>Your order is being prepared and will arrive soon!</Text>
    </View>
  );
};

// App Navigation
const AppNavigator = () => {
  return (
    <NavigationContainer theme={DarkTheme}>
      <Stack.Navigator initialRouteName="Home">
        <Stack.Screen name="Home" component={HomeScreen} />
        <Stack.Screen name="RestaurantDetails" component={RestaurantDetailsScreen} />
        <Stack.Screen name="Cart" component={CartScreen} />
        <Stack.Screen name="Checkout" component={CheckoutScreen} />
        <Stack.Screen name="OrderTracking" component={OrderTrackingScreen} />
      </Stack.Navigator>
    </NavigationContainer>
  );
};

// App Component
export default function App() {
  return (
    <PaperProvider theme={DarkTheme}>
      <AppNavigator />
    </PaperProvider>
  );
}

// Styles
const styles = StyleSheet.create({
  container: {
    flex: 1,
    padding: 20,
    backgroundColor: '#121212',
  },
  title: {
    color: '#fff',
    fontSize: 24,
    marginBottom: 20,
    fontWeight: 'bold',
  },
  card: {
    backgroundColor: '#1f1f1f',
    padding: 15,
    marginBottom: 10,
    borderRadius: 10,
    alignItems: 'center',
  },
  cardImage: {
    width: '100%',
    height: 150,
    borderRadius: 10,
    marginBottom: 10,
  },
  menuImage: {
    width: '100%',
    height: 100,
    borderRadius: 10,
    marginBottom: 10,
  },
  cardTitle: {
    color: '#fff',
    fontSize: 18,
    fontWeight: 'bold',
  },
  cardText: {
    color: '#fff',
    fontSize: 14,
  },
  menuItemName: {
    color: '#fff',
    fontSize: 16,
    marginBottom: 10,
  },
  input: {
    height: 40,
    backgroundColor: '#333',
    color: '#fff',
    marginBottom: 20,
    paddingHorizontal: 10,
    borderRadius: 5,
  },
});
